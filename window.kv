#:kivy 2.0
<RoundButtonvar@Button>:
    background_color: (0, 0, 0, 0)
    background_normal: ''
    canvas.before:
        Color:
            rgba:  (10/255, 111/255, 148/255, 1)
        RoundedRectangle:
            pos: self.pos
            size: self.size
            radius: [29]

